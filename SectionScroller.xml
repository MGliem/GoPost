<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
    <Frame name="SectionScrollerTemplate" virtual="true">
		<Scripts>
			<OnLoad>
				SectionScroller_OnLoad(self);
			</OnLoad>
			<OnMouseWheel>
				SectionScroller_OnMouseWheel(self, delta);
			</OnMouseWheel>
		</Scripts>
    </Frame>
</Ui>
