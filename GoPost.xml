<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
    
    <Button name="GPSmallButton" inherits="UIPanelButtonTemplate" virtual="true">
        <NormalFont style="GameFontNormalSmall" />
        <HighlightFont style="GameFontHighlightSmall" />
        <DisabledFont style="GameFontDisableSmall" />
    </Button>
    
    <Font name="GPHeaderFont" font="Fonts/ARIALN.TTF">
        <FontHeight><AbsValue val="14" /></FontHeight>
    </Font>
    
    <Font name="GPNumberFont" font="Fonts/ARIALN.TTF">
        <FontHeight><AbsValue val="13" /></FontHeight>
    </Font>
    
    <Font name="GPNumberFontSmaller" font="Fonts/ARIALN.TTF">
        <FontHeight><AbsValue val="12" /></FontHeight>
    </Font>

    <Font name="GPNumberFontTiny" font="Fonts/ARIALN.TTF">
        <FontHeight><AbsValue val="8" /></FontHeight>
    </Font>

    <Font name="GPFont" font="Fonts/FRIZQT__.TTF">
        <FontHeight><AbsValue val="11" /></FontHeight>
    </Font>

    <Font name="GPFontBigger" font="Fonts/FRIZQT__.TTF">
        <FontHeight><AbsValue val="12" /></FontHeight>
    </Font>

</Ui>
